# DeepSeek API 配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_MAX_TOKENS=2000
DEEPSEEK_BASE_URL=https://api.deepseek.com

# 应用配置
VITE_APP_TITLE=LLM Automation Demo
VITE_API_BASE_URL=http://localhost:3000

# Playwright 配置
PLAYWRIGHT_BROWSERS=0
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_VIDEO=true
PLAYWRIGHT_SCREENSHOT=true

# 测试配置
TEST_BASE_URL=https://r1bms.fuyoukache.com/
TEST_TIMEOUT=30000
TEST_RETRIES=2

# 账号配置（全局登录使用）
TEST_USERNAME=
TEST_PASSWORD=
# 可选覆盖：
# LOGIN_URL=https://r1ua.fuyoukache.com/view/admin/login

# MCP 兜底配置（可选）
# 启用 MCP 兜底：true/false（默认 false）
MCP_ENABLED=false
# MCP/OpenAI 兼容服务地址
MCP_ENDPOINT=
# 若服务需要鉴权（可选）
MCP_API_KEY=
# 模型名（按你的服务实际配置）
MCP_MODEL=gpt-4o-mini

# 业务路径（可选）
# 押金页面路径，若与你的菜单路由不同可在此覆盖
DEPOSIT_PAGE_PATH=/#/finance/deposit

# LLM 测试生成配置
LLM_GENERATE_TESTS=true
LLM_TEST_TEMPLATE=default
LLM_CONFIDENCE_THRESHOLD=0.8

# TAPD集成配置（可选）
# 如不配置，提Bug功能将使用本地模拟模式
TAPD_API_BASE=https://api.tapd.cn
TAPD_WORKSPACE_ID=
TAPD_USERNAME=
TAPD_API_TOKEN=
TAPD_DEFAULT_OWNER=
TAPD_ATTACH_UPLOAD=true 